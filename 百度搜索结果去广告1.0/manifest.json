{
  "name": "百度搜索结果去广告 - 大话主席",
  "version": "2.0",
  "manifest_version": 2,
  "description": "百度搜索结果去广告 - 大话主席",
  "icons": { "16": "images/logo16.png","48": "images/logo48.png","128": "images/logo128.png" },
  "permissions": ["tabs", "http://*/*", "https://*/*","notifications"],
  "background": { "scripts": ["background.js"] },
  "content_scripts": [{
      "matches": ["https://www.baidu.com/*"],
      "css": ["append.css"]
    }],
  "browser_action": {
    "default_icon": "images/logo48.png",
    "default_title" : "百度搜索结果去广告 - 大话主席"
  }
}